#include<stdio.h>
int main()
{
	int n,sum=0,square,rem=0,temp;
	printf("enter a num ");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
		rem=square%10;
		sum=sum+rem*rem*rem;
		n=n/10;
	}
	if(square==n)
	{
		printf("%d is the amstrong number",temp);
	}
	else
	{
		printf("%d is not a amstrong number",temp);
	}
}
